{
  "expo": {
    "name": "데사책방",
    "slug": "DSbookstore",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.shaby.DSbookstore",
      "infoPlist":{
        "NSPhotoLibraryUsageDescription":"사용자의 이미지를 게시물에 첨부하는 과정에 사용됩니다\nUsed in the process of attaching the user's image to the post"
      }
    },
    "android": {
      "package":"com.martini.DSbookstore",
      "versionCode":2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
