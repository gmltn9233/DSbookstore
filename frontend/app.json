{
  "expo": {
    "name": "DSbookstore",
    "slug": "DSbookstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.shaby.DSbookstore",
      "infoPlist":{
        "NSPhotoLibraryUsageDescription":"사용자의 이미지를 게시물에 첨부하는 과정에 사용됩니다"
      }
    },
    "android": {
      "package":"com.martini.DSbookstore",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#303D74"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
